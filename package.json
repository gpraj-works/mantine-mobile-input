{
  "name": "mantine-mobile-input",
  "version": "1.0.1",
  "description": "A mantine mobile/phone input component with country code",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json && tsc -p tsconfig.build.json --module ESNext --outDir dist/esm",
    "prepublishOnly": "npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0",
    "@mantine/core": ">=8.0.0 <9.0.0",
    "@mantine/hooks": ">=8.0.0 <9.0.0",
    "react-international-phone": "^4.2.2"
  },
  "keywords": [
    "mantine",
    "mantine-component",
    "react",
    "react-component",
    "phone-input",
    "mobile-input",
    "phone-number",
    "country-code",
    "international-phone",
    "typescript"
  ],
  "author": "Pushparaj Gunasekaran",
  "license": "MIT"
}
