{
  "name": "mantine-mobile-input",
  "version": "1.0.1",
  "description": "A mantine mobile/phone input component with country code",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "dev": "tsup src/index.ts --watch --dts --format esm,cjs --out-dir dist",
    "prepublishOnly": "npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "prettier": "^3.6.2",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7"
  },
  "dependencies": {},
  "peerDependencies": {
    "@mantine/core": ">=8.0.0 <9.0.0",
    "@mantine/hooks": ">=8.0.0 <9.0.0",
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0",
    "react-international-phone": "^4.2.2"
  },
  "keywords": [
    "mantine",
    "mantine-component",
    "mantine-ui",
    "react",
    "react-component",
    "react-input",
    "phone-input",
    "mobile-input",
    "phone-number",
    "country-code",
    "international-phone",
    "form-input",
    "typescript",
    "ui-component"
  ],
  "author": "Pushparaj Gunasekaran",
  "license": "MIT"
}
